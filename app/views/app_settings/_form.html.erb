<%= simple_form_for(@app_setting) do |f| %>
    <%= f.error_notification %>

    <div class="form-inputs">
      <%= f.input :Vrate, label: "Village Rate" %>
      <%= f.input :Irate, label: "Individual Rate" %>
      <%= f.input :LastInvNum, label: "Last Invoice Number" %>
    </div>

    <div class="form-actions">
      <%= submit_tag "Save Changes", :class => "btn btn-small btn-success" %>
    </div>
    <br>
<% end %>
