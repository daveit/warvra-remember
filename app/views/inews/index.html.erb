<p id="notice"><%= notice %></p>

<h1>Listing Inews</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>First</th>
      <th>Last</th>
      <th>Idate</th>
      <th>Description</th>
      <th>Status</th>
      <th>Total</th>
      <th>Subject</th>
      <th>Body</th>
      <th>Fromemail</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inews.each do |inews| %>
      <tr>
        <td><%= inews.title %></td>
        <td><%= inews.first %></td>
        <td><%= inews.last %></td>
        <td><%= inews.idate %></td>
        <td><%= inews.description %></td>
        <td><%= inews.status %></td>
        <td><%= inews.total %></td>
        <td><%= inews.subject %></td>
        <td><%= inews.body %></td>
        <td><%= inews.fromemail %></td>
        <td><%= link_to 'Show', inews %></td>
        <td><%= link_to 'Edit', edit_inews_path(inews) %></td>
        <td><%= link_to 'Destroy', inews, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Inews', new_inews_path %>
