<%= simple_form_for(@acc_account) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :code %>
    <%= f.association :acc_type, label: "Select Account Type" %>
    <%= f.input :numtran, label: "Number of Transactions" %>
    <%= f.input :balance, label: "Balance" %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
